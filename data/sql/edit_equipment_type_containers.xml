<!DOCTYPE query SYSTEM "__default_table_names__.txt">
<query name="edit_equipment_type_containers">
    <connection>&ifp_connection;</connection>
    <params>
        <param name="//_post/business_line_id" type="text"/>
        <param name="//_post/priority_id" type="text"/>
                <param name="//_post/equipment_type_container_id" type="text"/>
        <param name="php:date('Y-m-d H:i:s')" type="text"/>
    </params>
    <sql>
        UPDATE uplink_equipment_type_container SET default_business_line_id=?, default_priority_id=? WHERE account_id=? AND equipment_type_container_id=? AND deletion_datetime &gt; ?
	</sql>
</query>
