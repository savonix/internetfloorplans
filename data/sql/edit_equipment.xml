<query name="edit_equipment">
    <connection>savonix</connection>
    <params>
        <param name="_post/equipment_id" type="integer"/>
		<param name="php:$_SESSION['NX_AUTH']['account_id']" type="text"/>
        <param name="_post/asset_id" type="integer"/>
		<param name="_post/equipment_type_id" type="integer"/>
		<param name="_post/priority_id" type="text"/>
		<param name="_post/lob_id" type="text"/>
        <param name="php:date('Y-m-d H:i:s')" type="text"/>
	</params>
    <sql>
        INSERT INTO uplink_equipment (equipment_id,account_id,asset_id,equipment_type_id,priority_id,business_line_id,creation_datetime)
		VALUES (?,?,?,?,?,?,?)
    </sql>
</query>
