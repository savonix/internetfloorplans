<!DOCTYPE query SYSTEM "__default_table_names__.txt">
<query name="add_equipment_gif_image">
    <connection>&ifp_connection;</connection>
    <params>
		<param name="php:$_SESSION['NX_AUTH']['account_id']" type="text"/>
		<param name="_post/name" type="text"/>
        <param name="_post/equipment_type_id" type="text"/>
		<param name="_post/default" type="text"/>
		<param name="_files/file/name" type="text"/>
        <param name="//thumb_thumbname" type="text"/>
		<param name="_files/file/type" type="text"/>
		<param name="_post/comments" type="text"/>
        <param name="php:date('Y-m-d H:i:s')" type="text"/>
    </params>
    <sql>
        INSERT INTO uplink_equipment_type_images
		(equipment_type_image_id,account_id,name,equipment_type_id,default_image, image_pointer,thumb_pointer,mime_type,comments, creation_datetime) 
		VALUES (_AUTO_,?,?,?,?,?,?,?,?,?)
    </sql>
</query>
